
@{
    ViewBag.Title = "Schedule";
}

<h2>Schedule</h2>

<div class="row">
    <div class="col-lg-6 col-xs-12">
        <iframe src="https://calendar.google.com/calendar/embed?height=600&amp;wkst=1&amp;bgcolor=%23FFFFFF&amp;src=caseygruse222%40gmail.com&amp;color=%2329527A&amp;ctz=America%2FLos_Angeles" style="border-width:0" width="600" height="400" frameborder="0" scrolling="no"></iframe>
    </div>
