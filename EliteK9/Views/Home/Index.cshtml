@{
    ViewBag.Title = "Home Page";
}

<head>
    <link rel="stylesheet" type="text/css" href="~/Css/HomePageCss.css" />
</head>

<div class="row">
    <div class="col-sm-12">
        <div id="myCarousel" class="carousel slide center-block" data-ride="carousel" style="width: 80%; margin-top: 15px;" >

            <!-- Carousel indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>
            <!-- Wrapper for carousel items -->
            <div class="carousel-inner">
                <div class="item active">
                    <img class="centerCarousel center-block" src="~/images/test4.png" alt="First Slide">
                </div>
                <div class="item">
                    <img class="centerCarousel center-block" src="~/images/test5.png" alt="Second Slide">
                </div>
                <div class="item">
                    <img class="centerCarousel center-block" src="~/images/test3.png" alt="Third Slide">
                </div>
            </div>
            <!-- Carousel controls -->
            <a class="carousel-control left" href="#myCarousel" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"></span>
            </a>
            <a class="carousel-control right" href="#myCarousel" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"></span>
            </a>
        </div>
    </div>
</div>











